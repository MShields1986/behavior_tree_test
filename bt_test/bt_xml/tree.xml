<root BTCPP_format="4" >
  <BehaviorTree ID="MainTree">
    <Fallback name="root_fallback">
      <Sequence name="main_behavior">
        <SetBlackboard output_key="home_base_loc" value="map;0;0;0;0;0;0;1" />
        <Fallback name="go_to_home_fallback">
          <CheckPose name="check_pose" pose="{home_base_loc}"/>
          <BaseToGoal name="base_to_home" pose="{home_base_loc}"/>
        </Fallback>
        <Sequence name="go_to_fixture_sequence">
          <TfToPose name="tf_to_pose" frame_id="fixture_1" pose="{fixture_base_loc}"/>
          <Fallback name="go_to_fixture_fallback">
            <CheckPose name="check_pose" pose="{fixture_base_loc}"/>
            <BaseToGoal name="base_to_fixture" pose="{fixture_base_loc}"/>
          </Fallback>
        </Sequence>
      </Sequence>
      <AskForHelp name="ask_for_help"/>
    </Fallback>
  </BehaviorTree>
</root>