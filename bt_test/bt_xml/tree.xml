<root BTCPP_format="4" >
  <include path="./home.xml" />

  <BehaviorTree ID="MainTree">
    <Fallback name="root_fallback">
      <Sequence name="main_behavior">
        <SubTree ID="Home" />
        <Sequence name="base_to_fixture_sequence">
          <TfToPose name="tf_to_pose" frame_id="fixture_1" pose="{fixture_base_loc}"/>
          <Fallback name="base_to_fixture_fallback">
            <CheckPose name="base_check_fixture" pose="{fixture_base_loc}"/>
            <BaseToGoal name="base_to_fixture" pose="{fixture_base_loc}"/>
          </Fallback>
        </Sequence>
      </Sequence>
      <AskForHelp name="ask_for_help"/>
    </Fallback>
  </BehaviorTree>

</root>